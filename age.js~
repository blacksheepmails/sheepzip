function main() {
    if (localStorage.language == 'english') {
        document.getElementById('age-question').innerHTML = 'My age is:';
    } else {
        document.getElementById('age-question').innerHTML = "Rwy'n __ oed:";
    }
}

function clickHandler (age) {
	localStorage.age=age;
	window.location.href="gender.html";
}
