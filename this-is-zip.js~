function main() {
    if (localStorage.language == 'english') {
        document.getElementById("zip-text").innerHTML = 'This is Zip. Zip needs your help!';
    } else {
        document.getElementById("zip-text").innerHTML = 'Dyma Zip. Mae Zip angen dy help!';
    }
}

function next() {
	window.location.href="age.html";
}
